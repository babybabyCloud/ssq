{
    "version":1,
    "formatters": {
		"default": {
			"fmt":"%(asctime)s-%(levelname)s %(name)s - %(message)s"
		}
    },
    "handlers": {
        "console": {
            "class":"logging.StreamHandler",
            "formatter":"default",
            "level":"INFO",
            "stream":"ext://sys.stdout"
        },
        "file": {
            "class":"logging.FileHandler",
            "formatter":"default",
            "level":"DEBUG",
            "filename":"log",
			"encoding": "utf-8"
        }
    },
    "loggers": {
        "Spider": {
            "handlers":[
                "console",
                "file"
            ],
            "level": "INFO"
        }
    },
    "root": {
        "handlers":[
            "console"
        ],
        "level": "INFO"
    }
}
